{
    "title": "liqianlong's book",
    "author": "liqianlong",
    "description": "linux python",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "styles": {
        "website": "styles/website.css",
		"ebook": "styles/ebook.css",
		"pdf": "styles/pdf.css",
		"mobi": "styles/mobi.css",
		"epub": "styles/epub.css"
    },
    "structure": {
        "readme": "README.md",
		"summary":"SUMMARY.md"
    },
    "plugins": [
        "-sharing",
        "splitter",
        "highlight",
        "search",
        "tbfed-pagefooter",
        "anchor-navigation-ex",
        "-hide-element",
        "copy-code-button",
        "3-ba",
        "favicon"
    ],
    "pluginsConfig": {
        "3-ba": {
            "token": "a966d180803ca8cd07c967d6fdfb8d3f"
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        },
		"tbfed-pagefooter": {
			"copyright": "FROM liqianlong",
			"modify_label": "该文件修订时间：",
			"modify_format": "YYYY-MM-DD HH:mm:ss"
		},
		"favicon":{
            "shortcut": "./gitbook/images/favicon.ico",
            "bookmark": "./gitbook/images/favicon.ico",
            "appleTouch": "./gitbook/images/apple-touch-icon.jpg",
            "appleTouchMore": {
                "120x120": "./gitbook/images/apple-touch-icon.jpg",
                "180x180": "./gitbook/images/apple-touch-icon.jpg"
            }
		}
    }
}